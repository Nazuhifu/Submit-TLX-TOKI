#include<bits/stdc++.h>
using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int x,y;
    cin >> x >> y;
    for(int i = 1; i <= x; i++){
        if(i%y==0){
            cout << "* ";
        }
        else{
            cout << i << ' ';
        }
    }

    return 0;
}

#include<bits/stdc++.h>
using namespace std;

bool power2(int n){
	if(n==0)  return false;
	return (ceil(log2(n)) == floor(log2(n)));
}

int cnt,N;

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);
	cin >> N;
	power2(N)? cout << "ya" << '\n': cout << "bukan" << '\n';
	
	return 0;
}
